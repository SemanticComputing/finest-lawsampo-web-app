{
    "id": "caselaw",
    "endpoint": {
        "url": "https://ldf.fi/lawsampo/sparql",
        "useAuth": true,
        "prefixesFile": "SparqlQueriesPrefixes.js"
    },
    "sparqlQueriesFile": "SparqlQueriesCaselaw.js",
    "baseURI": "http://ldf.fi/lawsampo",
    "URITemplate": "<BASE_URI>/<LOCAL_ID>",
    "facetClass": "lss:Judgment",
    "langTag": "fi",
    "langTagSecondary": "sv",
    "frontPageImage": "lawsampo/case_law_cropped.jpg",
    "searchMode": "faceted-search",
    "defaultActiveFacets": [
        "jenaText"
    ],
    "defaultTab": "table",
    "resultClasses": {
        "caselaw": {
            "paginatedResultsConfig": {
                "tabID": 0,
                "component": "ResultTable",
                "tabPath": "table",
                "tabIcon": "CalendarViewDay",
                "propertiesQueryBlock": "judgementPropertiesFacetResults",
                "pagesize": 20,
                "sortBy": "decisionDate",
                "sortDirection": "desc",
                "postprocess": {
                    "func": "processAbstracts"
                }
            },
            "instanceConfig": {
                "propertiesQueryBlock": "judgementPropertiesInstancePage",
                "postprocess": {
                    "func": "processAbstracts"
                },
                "instancePageResultClasses": {
                    "instancePageTable": {
                        "tabID": 0,
                        "component": "InstancePageTable",
                        "tabPath": "table",
                        "tabIcon": "CalendarViewDay"  
                    },
                    "instancePageExport": {
                        "tabID": 1,
                        "component": "Export",
                        "tabPath": "export",
                        "tabIcon": "ArrowForward",
                        "pageType": "instancePage"
                    }
                }
            }
        },
        "judgementsByYear": {
            "tabID": 1,
            "component": "ApexCharts",
            "createChartData": "createSingleLineChartData",
            "stroke": {
                "curve": "straight",
                "width": 2
            },
            "xaxisTickAmount": 20,
            "title": "Oikeustapaukset vuosittain",
            "xaxisTitle": "Vuosi",
            "yaxisTitle": "Oikeustapausten lukumäärä",
            "tabPath": "by_year",
            "tabIcon": "ShowChart",
            "sparqlQuery": "judgementsByYearQuery",
            "facetClass": "caselaw",
            "filterTarget": "judgement",
            "resultMapper": "mapLineChart",
            "resultMapperConfig": {
              "fillEmptyValues": true
            }
        },
        "export": {
            "tabID": 2,
            "component": "Export",
            "tabPath": "export",
            "tabIcon": "ArrowForward",
            "resultClass": "caselaw",
            "facetClass": "caselaw"
        }
    },
    "properties": [
        {
            "id": "prefLabel",
            "valueType": "object",
            "makeLink": true,
            "externalLink": false,
            "sortValues": false,
            "numberedList": false
        },
        {
            "id": "ecli",
            "valueType": "string",
            "makeLink": false,
            "externalLink": false,
            "sortValues": false,
            "numberedList": false
        },
        {
            "id": "abstract",
            "valueType": "string",
            "collapsedMaxWords": 6,
            "makeLink": false,
            "externalLink": false,
            "sortValues": false,
            "numberedList": false,
            "minWidth": 300
        },
        {
            "id": "court",
            "valueType": "object",
            "makeLink": false,
            "externalLink": false,
            "sortValues": false,
            "numberedList": false
        },
        {
            "id": "typeOfSourceData",
            "onlyOnInstancePage": true,
            "valueType": "object",
            "makeLink": false,
            "externalLink": false,
            "sortValues": false,
            "numberedList": false
        },
        {
            "id": "decisionDate",
            "valueType": "string",
            "makeLink": false,
            "externalLink": false,
            "sortValues": false,
            "numberedList": false
        },
        {
            "id": "subjectCategory",
            "onlyOnInstancePage": true,
            "valueType": "object",
            "makeLink": true,
            "externalLink": true,
            "sortValues": false,
            "numberedList": false
        },
        {
            "id": "situationCategory",
            "onlyOnInstancePage": true,
            "valueType": "object",
            "makeLink": true,
            "externalLink": false,
            "sortValues": false,
            "numberedList": false
        },
        {
            "id": "similarCourtDecicions",
            "onlyOnInstancePage": true,
            "valueType": "object",
            "makeLink": true,
            "externalLink": false,
            "sortValues": false,
            "numberedList": false
        },
        {
            "id": "finlexLink",
            "valueType": "object",
            "onlyOnInstancePage": true,
            "makeLink": true,
            "externalLink": true,
            "sortValues": false,
            "numberedList": false
        },
        {
            "id": "uri",
            "valueType": "object",
            "onlyOnInstancePage": true,
            "makeLink": true,
            "externalLink": true,
            "sortValues": false,
            "numberedList": false
        },
        {
            "id": "contentHTMLAnnotated",
            "valueType": "string",
            "onlyOnInstancePage": true,
            "renderAsHTML": true,
            "HTMLParserTask": "addAnnotationTooltips",
            "makeLink": false,
            "externalLink": false,
            "sortValues": false,
            "numberedList": false
        }
    ],
    "facets": {
        "prefLabel": {
            "sortByPredicate": "skos:prefLabel"
        },
        "jenaText": {
            "containerClass": "one",
            "facetType": "text",
            "filterType": "textFilter",
            "textQueryPredicate": "lss:is_realized_by"
        },
        "court": {
            "containerClass": "five",
            "facetLabelFilter": "FILTER(LANG(?prefLabel_) = '<LANG>')",
            "facetType": "list",
            "filterType": "uriFilter",
            "pieChartButton": true,
            "predicate": "dcterms:creator",
            "sortBy": "instanceCount",
            "sortByPredicate": "dcterms:creator/skos:prefLabel",
            "sortDirection": "desc"
        },
        "decisionDate": {
            "containerClass": "ten",
            "facetType": "hierarchical",
            "filterType": "uriFilter",
            "parentProperty": "skos:broader",
            "predicate": "lss:timespan",
            "searchField": true,
            "sortByPredicate": "dcterms:issued"
        },
        "typeOfSourceData": {
            "containerClass": "four",
            "facetLabelFilter": "FILTER(LANG(?prefLabel_) = '<LANG>')",
            "facetType": "list",
            "filterType": "uriFilter",
            "pieChartButton": true,
            "predicate": "lss:material_type",
            "sortButton": true,
            "sortBy": "instanceCount",
            "sortByPredicate": "lss:material_type/skos:prefLabel",
            "sortDirection": "desc"
        },
        "typeOftheMatter": {
            "containerClass": "ten",
            "facetType": "list",
            "filterType": "uriFilter",
            "predicate": "lss:procedure",
            "searchField": true,
            "sortButton": true,
            "sortBy": "instanceCount",
            "sortByPredicate": "lss:procedure/skos:prefLabel",
            "sortDirection": "desc"
        },
        "subjectCategory": {
            "containerClass": "ten",
            "facetType": "list",
            "filterType": "uriFilter",
            "pieChartButton": true,
            "predicate": "dct:subject",
            "searchField": true,
            "sortBy": "instanceCount",
            "sortByPredicate": "dct:subject/skos:prefLabel",
            "sortDirection": "desc"
        },
        "situationCategory": {
            "containerClass": "ten",
            "facetType": "list",
            "filterType": "uriFilter",
            "pieChartButton": true,
            "predicate": "lss:situation_category",
            "searchField": true,
            "sortButton": true,
            "sortBy": "instanceCount",
            "sortByPredicate": "lss:situation_category/skos:prefLabel",
            "sortDirection": "desc"
        },
        "language": {
            "containerClass": "three",
            "facetType": "list",
            "filterType": "uriFilter",
            "literal": true,
            "pieChartButton": true,
            "predicate": "lss:is_realized_by/dcterms:language",
            "sortButton": true,
            "sortBy": "instanceCount",
            "sortByPredicate": "lss:is_realized_by/dcterms:language",
            "sortDirection": "desc"
        }
    }
}
